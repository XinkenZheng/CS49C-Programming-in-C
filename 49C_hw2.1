//program to convert Celsius to Fahrenheit and wise versa.
#include <stdio.h>
void celsius(void)
{
  for(int c = 0; c <= 100; ++c)
  {
    float temp;
    temp = (c *1.8 + 32); //formula for finding Fehrenheit
    printf("Celsius = %d  Fahrenheit = %.1f\n", c,temp);
  }
}

void fahrenheit(void)
{
  for(int f= 32; f <= 212; ++f)
  {
    float temp2;
    temp2 = (f-32) * 0.55555556; //formula for finding celsius
    printf("Fahrenheit = %d  Celsius = %.1f\n", f, temp2);
  }
}


int main(void) {
  celsius();
  fahrenheit();
}


